// calculate the binomial coefficient n over k
program recursionA
{
    int binom( int n, int k )
    {
        if( k == 0 ) { return 1; }
        if( k == 1 ) { return n; }
        return binom( n-1, k ) + binom( n-1, k-1 );
    }

