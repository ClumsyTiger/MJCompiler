class Root
{
    static int a;

    {
        // k-th root of x
        int root( int k, int x )
            int a_km1;
            int i, j;
        {
            i = 0;
            a = 1;
            do
            {
                j = 0; a_km1 = 1;
                do
                {
                    a_km1 = a_km1 * a;
                    j++;
                } while( j < k-1 );
                
                a = ( ( k-1 )*a + x/a_km1 ) / k;

                i++;
            } while( i < 20 );

            return a;
        }
    }
}

class Vector3D
{
    static Root root;
    static int a;

    int x, y, z;
    
    {
        void init()
        {
            if( root == null ) { root = new Root; }
        }

        // scalar multiplication of two 3D vectors
        int scmul( Vector3D v2 )
            Vector3D v1;
            int res;
        {
            init();
            v1 = this;
            res = root.root( 2, v1.x*v2.x + v1.y*v2.y + v1.z*v2.z );
            return res;
        }
    }
}

{
    void main()
        Vector3D v1, v2;
